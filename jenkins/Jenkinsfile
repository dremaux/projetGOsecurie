pipeline {
    agent any
    
    stages {
        stage ('Build') {
            steps {
                git url: 'https://github.com/dremaux/projetGOsecurie.git'
                withMaven {
                    bat "mvn"
                }
            } // withMaven will discover the generated Maven artifacts, JUnit Surefire & FailSafe reports and FindBugs reports
          }
        stage('Test') {
            steps {
                // dir("<Your path where scripts are present>")
                dir ("C:/HelloWorld"){
                // Execute your java file
                bat 'dir'
                }
            }
        }
        stage('Deliver') {
            steps {
                bat 'mvn'
            }
        }
    }
}
