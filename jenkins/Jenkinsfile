pipeline {
    agent any
    
    stages {
        stage ('Build') {
            steps {
                //url: 'https://github.com/dremaux/projetGOsecurie.git'
                git branch: 'main',
                credentialsId: '08e4a8ef-c2cf-4c3c-b704-5852224e98c4',
                url: 'git@github.com:dremaux/projetGOsecurie.git'
                withMaven {
                    bat "mvn"
                }
            } // withMaven will discover the generated Maven artifacts, JUnit Surefire & FailSafe reports and FindBugs reports
          }
        stage('Test') {
            steps {
                // dir("<Your path where scripts are present>")
                dir ("C:/HelloWorld"){
                // Execute your java file
                bat 'dir'
                }
            }
        }
        stage('Deliver') {
            steps {
                bat 'mvn'
            }
        }
    }
}
